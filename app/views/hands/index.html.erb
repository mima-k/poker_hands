<div class="container">
  <div class="title">
    <h1>Check a Poker Hand</h1>
  </div>
  <div class="contents">
    <div class="form">
      <%= form_with  url: "/" do |f| %>
        <%= f.text_field :card, class: "input", size: 30 %>
        <%= f.submit "Check", class: 'submit' %>
      <% end %>
    </div>
    <div class="result">
      <%= @hand.card %>
    </div>
  </div>
</div>